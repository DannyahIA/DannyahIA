{
    "name": "GitHub Dark Pro",
    "font": "'Inter', 'Segoe UI', system-ui, -apple-system, sans-serif",
    "fontMono": "'Fira Code', 'JetBrains Mono', 'Courier New', monospace",
    "radius": 12,
    "radiusLarge": 16,
    "spacing": 16,
    "shadow": "0 8px 32px rgba(0, 0, 0, 0.4)",
    "shadowHover": "0 12px 48px rgba(0, 0, 0, 0.5)",
    "padding": {
        "card": 24,
        "compact": 20,
        "mini": 16
    },
    "sizes": {
        "title": 24,
        "heading": 18,
        "body": 14,
        "small": 12,
        "mini": 10
    },
    "colors": {
        "background": "#0d1117",
        "backgroundSecondary": "#010409",
        "card": "#161b22",
        "cardHover": "#1c2128",
        "border": "#30363d",
        "borderLight": "#21262d",
        "text": "#e6edf3",
        "textSecondary": "#8b949e",
        "textMuted": "#6e7681",
        "accent": "#58a6ff",
        "accentHover": "#79c0ff",
        "success": "#3fb950",
        "successLight": "#2ea043",
        "warning": "#d29922",
        "warningLight": "#f0883e",
        "error": "#f85149",
        "errorLight": "#ff7b72",
        "danger": "#f85149",
        "purple": "#a371f7",
        "purpleLight": "#bc8cff",
        "pink": "#f778ba",
        "pinkLight": "#ff9cd7",
        "orange": "#f0883e",
        "blue": "#58a6ff",
        "green": "#3fb950",
        "red": "#f85149",
        "yellow": "#d29922",
        "cyan": "#39d0d8",
        "teal": "#39c5cf"
    },
    "gradients": {
        "primary": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
        "success": "linear-gradient(135deg, #3fb950 0%, #238636 100%)",
        "accent": "linear-gradient(135deg, #58a6ff 0%, #1f6feb 100%)",
        "purple": "linear-gradient(135deg, #a371f7 0%, #8957e5 100%)",
        "sunset": "linear-gradient(135deg, #f093fb 0%, #f5576c 100%)",
        "ocean": "linear-gradient(135deg, #4facfe 0%, #00f2fe 100%)",
        "fire": "linear-gradient(135deg, #fa709a 0%, #fee140 100%)",
        "emerald": "linear-gradient(135deg, #3fb950 0%, #56d364 100%)"
    },
    "languageColors": {
        "JavaScript": "#f1e05a",
        "TypeScript": "#3178c6",
        "Python": "#3572A5",
        "Java": "#b07219",
        "Go": "#00ADD8",
        "Rust": "#dea584",
        "C++": "#f34b7d",
        "C": "#555555",
        "C#": "#178600",
        "Ruby": "#701516",
        "PHP": "#4F5D95",
        "Swift": "#ffac45",
        "Kotlin": "#A97BFF",
        "Dart": "#00B4AB",
        "HTML": "#e34c26",
        "CSS": "#563d7c",
        "Shell": "#89e051",
        "Vue": "#41b883",
        "React": "#61dafb",
        "Svelte": "#ff3e00"
    },
    "animations": {
        "duration": "0.3s",
        "durationSlow": "0.6s",
        "easing": "cubic-bezier(0.4, 0, 0.2, 1)",
        "easingBounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)"
    }
}